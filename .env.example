# Deezer Configuration
DEEZER_ARL=your_deezer_arl_cookie_here

# Spotify Configuration  
SPOTIFY_USERNAME=your_spotify_username
SPOTIFY_PASSWORD=your_spotify_password
SPOTIFY_TOTP_SECRET=your_spotify_totp_secret

# YouTube Configuration (optional)
YOUTUBE_COOKIES_FILE=cookies.txt

# OpenAI Configuration (Primary)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-5
OPENAI_BASE_URL=
OPENAI_ORGANIZATION=

# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key

# Amazon Bedrock Configuration
BEDROCK_MODEL_ID=anthropic.claude-3-5-haiku-20241022-v1:0
BEDROCK_REGION=us-east-1

# Database Configuration
DATABASE_URL=sqlite:///music_agent.db

# Agent Configuration
AGENT_NAME=music-discovery-agent
AGENT_LOG_LEVEL=INFO
PRIMARY_MODEL_PROVIDER=openai
FALLBACK_MODEL_PROVIDER=bedrock

# API Rate Limiting
DEEZER_RATE_LIMIT=10
SPOTIFY_RATE_LIMIT=10
YOUTUBE_RATE_LIMIT=5

# Cache Configuration
CACHE_DIR=.cache
CACHE_TTL=3600